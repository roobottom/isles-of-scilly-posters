<!doctype html>
<html lang="en-gb" data-scroll="0">
    <head>
      <meta charset="utf-8">
      <meta http-equiv="X-UA-Compatible" content="IE=edge">
      <meta name="viewport" content="width=device-width, initial-scale=1">
      <title>{% block pageTitle %}{{ title }} -  Isles of Scilly Posters{% endblock %}</title>
      
      {% block stylesheets %}
      <link rel="stylesheet" href="/css/isles-of-scilly-posters.css">
      {% endblock %}
    </head>
    <body class="no-js {% block bodyClasses %}{% endblock %}">
      {% block body %}
      
      {% block accessibilityMenu %}
      <nav class="accessibility-menu container">
        <a href="#main">Skip to content</a>
      </nav>
      {% endblock %}

      {% block main %}
      <main class="container" id="main">

          {% block content %}
          
          {% endblock %}

      </main>
      {% endblock %}

      

      {% block scripts %}
      {% endblock %}

      {% endblock %}
    </body>
</html>